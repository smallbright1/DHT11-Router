    467          static void ProjectApp_ProcessRtosMessage( void )
    468          {
    469            osalQueue_t inMsg;
    470          
    471            if ( osal_queue_receive( OsalQueue, &inMsg, 0 ) == pdPASS )
    472            {
    473              uint8 cmndId = inMsg.cmnd;
    474              uint32 counter = osal_build_uint32( inMsg.cbuf, 4 );
    475          
    476              switch ( cmndId )
    477              {
    478                case CMD_INCR:
    479                  counter += 1;  /* Increment the incoming counter */
    480                                 /* Intentionally fall through next case */
    481          
    482                case CMD_ECHO:
    483                {
    484                  userQueue_t outMsg;
    485          
    486                  outMsg.resp = RSP_CODE | cmndId;  /* Response ID */
    487                  osal_buffer_uint32( outMsg.rbuf, counter );    /* Increment counter */
    488                  osal_queue_send( UserQueue1, &outMsg, 0 );  /* Send back to UserTask */
    489                  break;
    490                }
    491                
    492                default:
    493                  break;  /* Ignore unknown command */    
    494              }
    495            }
    496          }
